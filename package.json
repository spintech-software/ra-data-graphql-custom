{
    "name": "@spintech-software/ra-data-graphql-custom",
    "version": "0.1.1",
    "description": "A GraphQL custom data provider for react-admin",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:spintech-software/ra-data-graphql-custom.git"
    },
    "publishConfig": { "registry": "https://npm.pkg.github.com/" },
    "bugs": {
        "url": "https://github.com/spintech-software/ra-data-graphql-custom/issues"
    },
    "homepage": "https://github.com/marmelab/react-admin#readme",
    "authors": [
        "Gildas Garcia",
        "Sergey Petrunin"
    ],
    "keywords": [
        "reactjs",
        "react",
        "react-admin",
        "admin-on-rest",
        "rest",
        "graphql"
    ],
    "license": "MIT",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --outDir esm --module es2015 --watch",
        "test": "jest",
        "clear": "rimraf ./lib && rimraf ./esm"
    },
    "moduleDirectories": [
        "node_modules",
        "src"
    ],
    "dependencies": {
        "graphql-ast-types-browser": "~1.0.2",
        "graphql-tag": "^2.10.1",
        "lodash": "~4.17.5",
        "pluralize": "~7.0.0",
        "ra-data-graphql": "^3.4.2"
    },
    "peerDependencies": {
        "graphql": "^14.1.1",
        "ra-core": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "babel-jest": "^25.3.0",
        "connected-react-router": "^6.5.2",
        "cross-env": "^5.2.0",
        "final-form": "^4.18.5",
        "final-form-arrays": "^3.0.2",
        "graphql": "^14.1.1",
        "history": "^4.7.2",
        "immutable": "^3.8.1",
        "jest": "^25.3.0",
        "jsonexport": "^2.4.1",
        "ra-core": "^3.4.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-final-form": "^6.3.3",
        "react-redux": "^7.1.0",
        "react-router": "^5.1.0",
        "react-router-dom": "^5.1.0",
        "redux": "^4.0.3",
        "redux-saga": "^1.0.0",
        "rimraf": "^2.6.3",
        "seamless-immutable": "^7.1.3",
        "typescript": ">=3.7.0"
    }
}
